{
  "manifest_version": 2,
  "name": "Typo Resolver",
  "version": "1.0",
  "default_locale": "en",
  "description": "Fix some typo",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Typo Resolver",
    "default_popup": "popup.html"
  },
  "author": "kewang ( cpckewang@gmail.com )",
  "homepage_url": "https://github.com/kewangtw/typo-resolver",
  "permissions": [
    "activeTab", "<all_urls>" 
  ]
}
