{
  "manifest_version": 2,
  "name": "Typo Resolver",
  "version": "0.7",
  "description": "Help you to feedback some typo",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["jquery.js", "background.js"]
  },
  "author": "kewang ( cpckewang@gmail.com )",
  "homepage_url": "https://github.com/g0v/typo-resolver",
  "permissions": [
    "contextMenus",
    "activeTab",
    "tabs",
    "<all_urls>"
  ]
}
