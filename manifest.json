{
  "name": "Typo Resolver",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["jquery.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Fix some typo",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
